{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^node:", "^@?\\w", "^@/.*", "^@/config", "^import type", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
